{
    "name": "arnaudmoncondhuy/synapse-core",
    "type": "symfony-bundle",
    "description": "Headless AI orchestration engine for Symfony â€” LLM clients, agents, memory, and storage.",
    "license": "PolyForm-Noncommercial-1.0.0",
    "keywords": ["symfony", "ai", "llm", "chatbot", "gemini", "openai", "bundle", "headless"],
    "authors": [
        {
            "name": "Arnaud Moncondhuy",
            "email": "arnaudmoncondhuy@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "doctrine/doctrine-bundle": "^2.10 || ^3.0",
        "doctrine/orm": "^2.19 || ^3.0",
        "symfony/doctrine-messenger": "^7.0 || ^8.0",
        "symfony/framework-bundle": "^7.0 || ^8.0",
        "symfony/http-client": "^7.0 || ^8.0",
        "symfony/messenger": "^7.0 || ^8.0",
        "symfony/security-bundle": "^7.0 || ^8.0",
        "symfony/uid": "^7.0 || ^8.0",
        "symfony/yaml": "^7.0 || ^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.93",
        "symfony/process": "^8.0",
        "symfony/test-pack": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "ArnaudMoncondhuy\\SynapseCore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArnaudMoncondhuy\\SynapseCore\\Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
